<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>User key</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for profiles <- @profiles do %>
    <tr>
      <td><%= profiles.name %></td>
      <td><%= profiles.description %></td>
      <td><%= profiles.user_key %></td>

      <td>
        <span><%= link "Show", to: Routes.profiles_path(@conn, :show, profiles) %></span>
        <span><%= link "Edit", to: Routes.profiles_path(@conn, :edit, profiles) %></span>
        <span><%= link "Delete", to: Routes.profiles_path(@conn, :delete, profiles), method: :delete, data: [confirm: "Are you sure?"] %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Profiles", to: Routes.profiles_path(@conn, :new) %></span>
